{
    "openapi": "3.0.0",
    "info": {
        "description": "Wellie application API",
        "title": "Wellie API",
        "contact": {},
        "version": "1.0"
    },
    "paths": {
        "/auth/telegram": {
            "post": {
                "description": "Authenticate user with Telegram init data",
                "tags": [
                    "auth"
                ],
                "summary": "Authenticate with Telegram",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/wellie_internal_contract.AuthTelegramRequest"
                            }
                        }
                    },
                    "description": "Telegram init data query",
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/wellie_internal_contract.AuthTelegramResponse"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid request or init data",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/wellie_internal_contract.ErrorResponse"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Invalid init data",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/wellie_internal_contract.ErrorResponse"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Server error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/wellie_internal_contract.ErrorResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/v1/me": {
            "get": {
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "description": "Retrieves the current user based on the JWT token",
                "tags": [
                    "user"
                ],
                "summary": "Get current user",
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/wellie_internal_db.User"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/wellie_internal_contract.ErrorResponse"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Server error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/wellie_internal_contract.ErrorResponse"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/v1/physical-stats": {
            "post": {
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "description": "Save physical statistics for the authenticated user",
                "tags": [
                    "physical-stats"
                ],
                "summary": "Save user physical stats",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/wellie_internal_contract.SavePhysicalStatsRequest"
                            }
                        }
                    },
                    "description": "Physical stats data",
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/wellie_internal_contract.SavePhysicalStatsResponse"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Invalid request",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/wellie_internal_contract.ErrorResponse"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/wellie_internal_contract.ErrorResponse"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Server error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/wellie_internal_contract.ErrorResponse"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "servers": [
        {
            "url": "http://https://api-wellie.mxksimdev.com"
        },
        {
            "url": "https://https://api-wellie.mxksimdev.com"
        }
    ],
    "components": {
        "securitySchemes": {
            "BearerAuth": {
                "description": "JWT Authorization header using Bearer scheme. Example: \"Authorization: Bearer {token}\"",
                "type": "apiKey",
                "name": "Authorization",
                "in": "header"
            }
        },
        "schemas": {
            "wellie_internal_contract.AuthTelegramRequest": {
                "type": "object",
                "properties": {
                    "query": {
                        "type": "string"
                    }
                }
            },
            "wellie_internal_contract.AuthTelegramResponse": {
                "type": "object",
                "properties": {
                    "token": {
                        "type": "string"
                    },
                    "user": {
                        "$ref": "#/components/schemas/wellie_internal_db.User"
                    }
                }
            },
            "wellie_internal_contract.ErrorResponse": {
                "type": "object",
                "properties": {
                    "error": {
                        "type": "string"
                    }
                }
            },
            "wellie_internal_contract.SavePhysicalStatsRequest": {
                "type": "object",
                "properties": {
                    "body_fat_percentage": {
                        "type": "number"
                    },
                    "gender": {
                        "type": "string"
                    },
                    "height": {
                        "type": "number"
                    },
                    "weight": {
                        "type": "number"
                    }
                }
            },
            "wellie_internal_contract.SavePhysicalStatsResponse": {
                "type": "object",
                "properties": {
                    "user": {
                        "$ref": "#/components/schemas/wellie_internal_db.User"
                    }
                }
            },
            "wellie_internal_db.PhysicalStats": {
                "type": "object",
                "properties": {
                    "body_fat_percentage": {
                        "type": "number"
                    },
                    "gender": {
                        "type": "string"
                    },
                    "height": {
                        "type": "number"
                    },
                    "weight": {
                        "type": "number"
                    }
                }
            },
            "wellie_internal_db.User": {
                "type": "object",
                "properties": {
                    "avatar_url": {
                        "type": "string"
                    },
                    "created_at": {
                        "type": "string"
                    },
                    "deleted_at": {
                        "type": "string"
                    },
                    "id": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "physical_stats": {
                        "$ref": "#/components/schemas/wellie_internal_db.PhysicalStats"
                    },
                    "telegram_id": {
                        "type": "integer"
                    },
                    "updated_at": {
                        "type": "string"
                    },
                    "username": {
                        "type": "string"
                    }
                }
            }
        }
    }
}